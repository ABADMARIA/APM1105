    "def iterative_formula(x):\n",
    "    return (20 - 5*x)**(1/3)\n",
    "\n",
    "x = 2\n",
    "while True:\n",
    "    print(f\"x = {x:.3f}\")\n",
    "    xn = iterative_formula(x)\n",
    "    if abs(xn - x) < 0.001:\n",
    "        break\n",
    "    x = xn\n",
    "\n",
    "print(f\"x = {x:.3f}\")\n"
   ]
